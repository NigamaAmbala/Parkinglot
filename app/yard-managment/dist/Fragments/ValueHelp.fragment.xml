<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog1"
		title="Parking lots"
		items="{
        path: '/Parkinglot',
        filters: [{ path: 'parkingType', operator: 'EQ', value1: true }]}"
		search=".onValueHelpDialogSearch1"
		confirm=".onValueHelpDialogClose1"
		cancel=".onValueHelpDialogClose1">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{lotId}"
            description="{lotId}"
             />
	</SelectDialog>
</core:FragmentDefinition>